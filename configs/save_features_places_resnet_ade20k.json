{
    "name": "",
    "n_gpu": 1,
    "method": "predict",
    "arch": {
        "type": "",
        "args": {

        }
    },
    "dataset": {
        "type": "ADE20KDataset",
        "args": {
            "path": "data/ade20k/ade20k_imagelabels.pth",
            "normalize": true,
            "means": [0.485, 0.456, 0.406],
            "stds": [0.229, 0.224, 0.225],
            "resize": [256, 256],
            "center_crop": [224, 224]
        }
    },
    "data_loader": {
        "args":{
            "batch_size": 128,
            "num_workers": 8
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 1e-3,
            "weight_decay": 0,
            "amsgrad": false
        }
    },
    "trainer": {
        "epochs": 30,

        "save_dir": "saved/places_resnet_ade20k_features/debug",
        "save_period": 1,
        "verbosity": 2,

        "monitor": "min val_loss",
        "early_stop": 10,

        "tensorboard": true
    }
}
